<script type="text/javascript">
$(function() {

	function do_redirect(next_url) {
		location.href = next_url
	};

	var form = $('form[name="captcha_form"]');

	form.ajaxForm(function(json){
		if (typeof(json)!='object'){
			location.reload();
		} else {
			if (json.success) {
				if (json.current_url!=undefined){
					location.href = json.current_url;
				};
				if (json.next_url!=undefined){
					setTimeout(function(){ location.href = json.next_url }, 3500);
				};
				show_message_dialog(json.message, 'success');
			} else {
				process_field_errors(json);
			}
		}
	});

    $('input[name="captcha"]').focus().keyup(function(e){
		var code = (e.keyCode ? e.keyCode: e.which);
		if (code==13) {
			$('input#captcha_submit').trigger('click')
		}
	});

	$('a#captcha').click(function(event){
		$('input#captcha_submit').trigger('click')
	})

	update_captcha();

});
</script>

